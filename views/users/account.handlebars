<div id="account" class="col s12 valign-wrapper">
  <div class="row">
    <div class="col s6">
      <div class="card">
        <div class="card-content center-align">
          <img src="/assets/images/profile-image-1.png" class="responsive-img circle" width="128px" alt="">
          <p class="m-t-lg flow-text">{{account.profile.first_name}} {{account.profile.last_name}}</p>
          <div class="chip m-t-sm blue-grey white-text">{{account.user.type}}</div>
        </div>
      </div>
    </div>
    <div class="col s6">
      <div class="row">
        {{#each account.profile.address}}
        <div class="card">
          <div class="card-content ">
            <span class="card-title">Address</span>
            <address>
              {{street_address}}
              <br>
              {{city}}, {{state}}
            </address>
          </div>
        </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
